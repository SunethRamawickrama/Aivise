generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Folder {
  id        String   @id @default(uuid())
  name      String
  fileUrl   String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  userId    String  // from clerk auth 
}
